{
	"name" : "kocuj/di",
	"description" : "Container for design pattern Dependency Injection in PHP 7 and 8",
	"type" : "library",
	"license" : "MIT",
	"homepage" : "http://libs.kocuj.pl/php-libraries/kocuj-di/",
	"authors" : [{
			"name" : "Dominik Kocuj",
			"role" : "Developer",
			"homepage" : "http://kocuj.pl"
		}
	],
	"require" : {
		"php" : "^7.4",
		"psr/container" : "~2.0",
		"myclabs/php-enum" : "~1.8"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.4.*",
		"phpdocumentor/phpdocumentor" : "~2.9"
	},
	"autoload" : {
		"psr-4" : {
			"Kocuj\\Di\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Kocuj\\Di\\Examples\\" : "examples",
			"Kocuj\\Di\\Examples\\Lib\\" : "examples/lib",
			"Kocuj\\Di\\Tests\\" : "tests"
		}
	},
	"keywords" : [
		"di-container",
		"dependency-injection",
		"php",
		"psr-1",
		"psr-4",
		"psr-11",
		"psr-12",
		"php7",
		"oop",
		"container"
	]
}
